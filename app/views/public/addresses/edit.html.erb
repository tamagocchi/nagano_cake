

<h3>配送先編集</h3>

<div class="delivery_edit">
  <%= form_with model: @address, local:true do |f| %>
    <h4>郵便番号（ハイフンなし）</h4>
    <%= f.text_field :postcode %>
    <h4>住所</h4>
    <%= f.text_field :destination %>
    <h4>宛名</h4>
    <%= f.text_field :delivery_name %>
    <input type="submit" class='btn btn-success' value='変更内容を保存'>
  <% end %>
</div>